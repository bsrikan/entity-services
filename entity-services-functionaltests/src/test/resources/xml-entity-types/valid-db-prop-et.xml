<es:model xmlns:es="http://marklogic.com/entity-services">
  <es:info>
      <es:title>Lets-test-DB-prop-generate</es:title>
      <es:version>0.0.1</es:version>
      <es:base-uri>http://marklogic.com/test</es:base-uri>
  </es:info>
  <es:definitions>
    <Customer>
     <es:properties>
        <CustomerId>
          <es:datatype>integer</es:datatype>
          <es:collation>http://marklogic.com/collation/en</es:collation>
        </CustomerId>
        <CompanyName>
          <es:datatype>string</es:datatype>
        </CompanyName>
        <Country>
           <es:datatype>boolean</es:datatype>
        </Country>
        <ContactName>
           <es:datatype>iri</es:datatype>
        </ContactName>
        <Liking><es:datatype>anyURI</es:datatype></Liking>
        <Age><es:datatype>double</es:datatype></Age>
        <Height><es:datatype>int</es:datatype></Height>
        <Weight><es:datatype>short</es:datatype></Weight>
        <YearsofService><es:datatype>gYearMonth</es:datatype></YearsofService>
        <Numberyrs><es:datatype>byte</es:datatype></Numberyrs>
        <Awards><es:datatype>float</es:datatype></Awards>
        <dateofShipment><es:datatype>date</es:datatype></dateofShipment>
        <dateofOrder><es:datatype>dateTime</es:datatype></dateofOrder>
        <CustSince><es:datatype>dayTimeDuration</es:datatype></CustSince>
        <DaysofSurfing><es:datatype>nonNegativeInteger</es:datatype></DaysofSurfing>
        <ProductsPurchased>
             <es:datatype>array</es:datatype>
             <es:items>
		<es:ref>#/definitions/Product</es:ref>
	     </es:items>
	   </ProductsPurchased>
       </es:properties>
       <es:primary-key>CustomerId</es:primary-key>
       <es:range-index>CompanyName</es:range-index>
       <es:range-index>Country</es:range-index>
       <es:range-index>CustomerId</es:range-index>
       <es:range-index>ContactName</es:range-index>
       <es:range-index>Weight</es:range-index>
       <es:range-index>Height</es:range-index>
       <es:range-index>Numberyrs</es:range-index>
       <es:range-index>Awards</es:range-index>
       <es:range-index>Age</es:range-index>
       <es:range-index>dateofShipment</es:range-index>
       <es:range-index>dateofOrder</es:range-index>
       <es:range-index>CustSince</es:range-index>
       <es:range-index>DaysofSurfing</es:range-index>
       <es:range-index>ProductsPurchased</es:range-index>
       <es:word-lexicon>CustomerId</es:word-lexicon>
       <es:word-lexicon>Age</es:word-lexicon>
       <es:word-lexicon>Weight</es:word-lexicon>
       <es:word-lexicon>DaysofSurfing</es:word-lexicon>
       <es:required>CustomerId</es:required>
     </Customer>
     <Product>
        <es:properties>
         <ProductName>
            <es:datatype>string</es:datatype>
         </ProductName> 
         <UnitPrice>
            <es:datatype>integer</es:datatype>
            <es:collation>http://marklogic.com/collation/en</es:collation>
         </UnitPrice>
         <SupplierID>
             <es:datatype>integer</es:datatype>
         </SupplierID>
         <Discontinued>
             <es:datatype>boolean</es:datatype>
         </Discontinued>
        </es:properties>
        <es:primary-key>ProductName</es:primary-key>
       <es:range-index>UnitPrice</es:range-index>
       <es:range-index>Discontinued</es:range-index>
       <es:word-lexicon>SupplierID</es:word-lexicon>
      </Product>
 </es:definitions>
</es:model>
