<es:model xmlns:es="http://marklogic.com/entity-services">
  <es:info>
    <es:title>ET-Prop-Not-Distinct</es:title>
    <es:version>0.0.1</es:version>
    <es:base-uri>http://marklogic.com/testing-entity-type</es:base-uri>
    <es:description>Model where ET and property names are not distinct</es:description>
  </es:info>
  <es:definitions>
      <Order>
         <es:properties>
            <OrderID>
               <es:datatype>integer</es:datatype>
            </OrderID>
            <productName>
               <es:ref>#/definitions/OrderDetails</es:ref>
            </productName>
            <OrderDate>
               <es:datatype>dateTime</es:datatype>
            </OrderDate>
            <ShipAddress>
               <es:datatype>string</es:datatype>
            </ShipAddress>
            <OrderDetails>
               <es:datatype>array</es:datatype>
               <es:items>
                  <es:ref>#/definitions/OrderDetails</es:ref>
               </es:items>
            </OrderDetails>
         </es:properties>
         <es:primary-key>OrderID</es:primary-key>
         <es:required>OrderID</es:required>
         <es:range-index>OrderDate</es:range-index>
         <es:range-index>productName</es:range-index>
         <es:range-index>OrderID</es:range-index>
      </Order>
      <OrderDetails>
          <es:properties>
            <quantity>
               <es:datatype>integer</es:datatype>
            </quantity>
            <productName>
               <es:datatype>string</es:datatype>
            </productName>
         </es:properties>
         <es:primary-key>quantity</es:primary-key>
      </OrderDetails>
  </es:definitions>
</es:model>