<es:entity-type xmlns:es="http://marklogic.com/entity-services">
  <es:info>abcd
  </es:info>
  <es:definitions>
    <Customer>
     <es:properties>
        <CustomerId>
          <es:datatype>string</es:datatype>
        </CustomerId>
        <CompanyName>
          <es:datatype>string</es:datatype>
        </CompanyName>
        <Country>
           <es:datatype>string</es:datatype>
        </Country>
        <ContactName>
           <es:datatype>string</es:datatype>
        </ContactName>
       </es:properties>
       <es:primary-key>CustomerId</es:primary-key>
     </Customer>
     <Product>
        <es:properties>
         <ProductName>
            <es:datatype>string</es:datatype>
         </ProductName> 
         <UnitPrice>
            <es:datatype>integer</es:datatype>
         </UnitPrice>
         <SupplierID>
             <es:datatype>integer</es:datatype>
         </SupplierID>
         <Discontinued>
             <es:datatype>boolean</es:datatype>
         </Discontinued>
        </es:properties>
      </Product>
 </es:definitions>
</es:entity-type>
