<search:constraint name="OrderID">
<search:range type="xs:decimal" facet="true">
<search:element xmlns:es="http://marklogic.com/entity-services">
</search:element>
</search:range>
</search:constraint>
<search:constraint name="OrderDetails">
<search:word>
<search:element ns="" name="OrderDetails">
</search:element>
</search:word>
</search:constraint>
<search:tuples name="Customer">
<search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/en">
<search:path-index xmlns:es="http://marklogic.com/entity-services" xmlns:cust="http://marklogic.com/customer">//es:instance/cust:Customer/cust:CustomerId</search:path-index>
</search:range>
<search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/en">
<search:path-index xmlns:es="http://marklogic.com/entity-services" xmlns:cust="http://marklogic.com/customer">//es:instance/cust:Customer/cust:CompanyName</search:path-index>
</search:range>
<search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/en">
<search:element ns="http://marklogic.com/customer" name="Country" xmlns:es="http://marklogic.com/entity-services">
</search:element>
</search:range>
<search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/en">
<search:element ns="http://marklogic.com/customer" name="Address" xmlns:es="http://marklogic.com/entity-services">
</search:element>
</search:range>
</search:tuples>
<search:tuples name="Product">
<search:range type="xs:decimal" facet="true">
<search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/Product/UnitPrice</search:path-index>
</search:range>
<search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/en">
<search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/Product/Discontinued</search:path-index>
